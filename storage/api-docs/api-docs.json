            "post": {
                "tags": [
                    "Products"
                ],
                "summary": "Crear nuevo producto",
                "operationId": "05b2b01e746b59356779b1a7966b18d1",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "description",
                                    "tax_cost",
                                    "manufacturing_cost",
                                    "price",
                                    "currency"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "Producto 1"
                                    },
                                    "description": {
                                        "type": "string",
                                        "example": "Descripción del producto 1"
                                    },
                                    "tax_cost": {
                                        "type": "number",
                                        "format": "float",
                                        "example": "15.5"
                                    },
                                    "manufacturing_cost": {
                                        "type": "number",
                                        "format": "float",
                                        "example": "50.0"
                                    },
                                    "price": {
                                        "required": [
                                            "value"
                                        ],
                                        "properties": {
                                            "value": {
                                                "type": "number",
                                                "format": "float",
                                                "example": 750
                                            }
                                        },
                                        "type": "object"
                                    },
                                    "currency": {
                                        "required": [
                                            "id",
                                            "name",
                                            "symbol",
                                            "exchange_rate"
                                        ],
                                        "properties": {
                                            "id": {
                                                "type": "integer",
                                                "example": "1"
                                            },
                                            "name": {
                                                "type": "string",
                                                "example": "US Dollar"
                                            },
                                            "symbol": {
                                                "type": "string",
                                                "example": "$"
                                            },
                                            "exchange_rate": {
                                                "type": "number",
                                                "format": "float",
                                                "example": 1
                                            }
                                        },
                                        "type": "object"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Producto creado exitosamente",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "id": {
                                            "type": "integer",
                                            "example": 10
                                        },
                                        "name": {
                                            "type": "string",
                                            "example": "Laptop X"
                                        },
                                        "price": {
                                            "properties": {
                                                "value": {
                                                    "type": "number",
                                                    "example": 750
                                                }
                                            },
                                            "type": "object"
                                        },
                                        "currency": {
                                            "properties": {
                                                "id": {
                                                    "type": "integer",
                                                    "example": 1
                                                },
                                                "name": {
                                                    "type": "string",
                                                    "example": "USD"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Datos inválidos"
                    },
                    "401": {
                        "description": "No autorizado"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
